class T1:
    n = 10
    def total(self,N):
        self.total = int(self.n)+int(N)

class T2:
    def total(self,s):
        self.total = len(str(s))

class T3:
    def total(self,N):
        self.total = int(N)/5

t1 = T1()
t2 = T2()
t3 = T3()

#main
t1.total(45)
t2.total(45)
t3.total(45)

print (t1.total)
print (t2.total)
print (t3.total)
