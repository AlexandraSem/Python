print("\n\nCounting:")
for i in range(10):
    print(i, end=" ")
print("\n\nCounting by files")
for i in range(0,50,5):
    print(i, end=" ")
print("\n\nCounting backwards")
for i in range(10,0,-1):
    print(i, end=" ") 
------
i = 1
for color in 'red','yellow','green','cyan','blue','violet':
    print('',i,'color of rainbow is',color,sep =' ')
------
for i in 1,2,3,'one','two','three':
    print(i,',',sep ='') 
